<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <welcome-file-list>
    <welcome-file>pages/frame.jsp</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>webAppRootKey</param-name>
    <param-value>ycszhyw.root</param-value>
  </context-param>
  <context-param>
    <param-name>log4jRefreshInterval</param-name>
    <param-value>60000</param-value>
  </context-param>
  <listener>
    <listener-class>org.springframework.web.util.Log4jConfigListener</listener-class>
  </listener>
  <context-param>
    <param-name>log4jConfigLocation</param-name>
    <param-value>/WEB-INF/classes/log4j.properties</param-value>
  </context-param>
  <context-param>
    <param-name>contextConfigLocation</param-name>
    <param-value>classpath:applicationContext.xml</param-value>
  </context-param>
  <listener>
    <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
  </listener>
  <filter>
    <filter-name>hibernateFilter</filter-name>
    <filter-class>org.springframework.orm.hibernate3.support.OpenSessionInViewFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>hibernateFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>
			org.springframework.web.util.IntrospectorCleanupListener
		</listener-class>
  </listener>
  <filter>
    <filter-name>encodingFilter</filter-name>
    <filter-class>org.springframework.web.filter.CharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>encodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>sysFilter</filter-name>
    <filter-class>com.ycszh.filter.SysFilter</filter-class>
    <init-param>
      <param-name>exclude</param-name>
      <param-value>/|*.css|*.js|*.gif|*.png|*.jpg</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>sysFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>struts2</filter-name>
    <filter-class>org.apache.struts2.dispatcher.ng.filter.StrutsPrepareAndExecuteFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>struts2</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>dwr-invoker</servlet-name>
    <servlet-class>org.directwebremoting.servlet.DwrServlet</servlet-class>
    <init-param>
      <param-name>debug</param-name>
      <param-value>true</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>dwr-invoker</servlet-name>
    <url-pattern>/dwr/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>imageServlet</servlet-name>
    <servlet-class>com.ycszh.servlet.ImageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>imageServlet</servlet-name>
    <url-pattern>/servlet/getimage</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>createBazServlet</servlet-name>
    <servlet-class>com.ycszh.servlet.CreateBazServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>createBazServlet</servlet-name>
    <url-pattern>/servlet/createBazServlet</url-pattern>
  </servlet-mapping> 
 
  <servlet>
    <servlet-name>jsrDzdaServlet</servlet-name>
    <servlet-class>com.ycszh.servlet.JsrDzdaImageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>jsrDzdaServlet</servlet-name>
    <url-pattern>/servlet/jsrDzdaServlet</url-pattern>
  </servlet-mapping>
  
  
    <servlet>
    <servlet-name>yzbmServlet</servlet-name>
    <servlet-class>com.ycszh.servlet.CreateYzbmServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>yzbmServlet</servlet-name>
    <url-pattern>/servlet/yzbmServlet</url-pattern>
  </servlet-mapping>   
  
   <servlet>
    <servlet-name>printImageServlet</servlet-name>
    <servlet-class>com.ycszh.servlet.PrintImageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>printImageServlet</servlet-name>
    <url-pattern>/servlet/PrintImageServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>zzsxphzImageServlet</servlet-name>
    <servlet-class>com.ycszh.servlet.ZzsxphzImageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>zzsxphzImageServlet</servlet-name>
    <url-pattern>/servlet/zzsxphzImageServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>sfrzcjImageServlet</servlet-name>
    <servlet-class>com.ycszh.servlet.SfrzcjImageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sfrzcjImageServlet</servlet-name>
    <url-pattern>/servlet/sfrzcjImageServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>jsrxpkServlet</servlet-name>
    <servlet-class>com.ycszh.servlet.JsrxpkServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>jsrxpkServlet</servlet-name>
    <url-pattern>/servlet/jsrxpkServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>InitSys</servlet-name>
    <servlet-class>com.ycszh.servlet.InitSys</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InitSys</servlet-name>
    <url-pattern>/initSys</url-pattern>
  </servlet-mapping>
  
  <servlet>
		<servlet-name>CreateBarCode</servlet-name>
		<servlet-class>com.ycszh.util.barcode.BarCodeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateBarCode</servlet-name>
		<url-pattern>/CreateBarCode</url-pattern>
	</servlet-mapping>
  
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <error-page>
    <error-code>404</error-code>
    <location>/pages/errorpage/pageNotFound.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/pages/errorpage/systemError.jsp</location>
  </error-page>
  <login-config>
    <auth-method>BASIC</auth-method>
  </login-config>
</web-app>